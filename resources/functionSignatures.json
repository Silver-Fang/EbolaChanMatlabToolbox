{
	"ParseRepeatingFlagArguments": {
		"inputs": [
			{
				"name": "InputFlags",
				"kind": "required",
				"type": [
					"row",
					"cell"
				],
				"purpose": "调用方传来的，待分析的旗帜类重复参数"
			},
			{
				"name": "ValidFlags",
				"kind": "required",
				"type": [
					"scalar",
					"string"
				],
				"repeating": true,
				"purpose": "函数规定的有效旗帜"
			}
		],
		"outputs": [
			{
				"name": "varargout",
				"type": [
					"scalar",
					"logical"
				],
				"multiplicity": "append"
			}
		]
	},
	"ColorAllocate": {
		"inputs": [
			{
				"name": "NoColors",
				"kind": "required",
				"type": [
					"scalar",
					"uint8"
				],
				"purpose": "要分配的颜色个数"
			},
			{
				"name": "TryCount",
				"kind": "ordered",
				"type": [
					"scalar",
					"uint8"
				],
				"purpose": "尝试优化的次数",
				"default": 1
			}
		],
		"outputs": [
			{
				"name": "Colors",
				"type": [
					"size=:,3"
				]
			}
		]
	},
	"DimensionFun": {
		"inputs": [
			{
				"name": "Function",
				"kind": "required",
				"type": "function_handle",
				"purpose": "要执行的函数。必须接受等同于Arguments重复次数的参数"
			},
			{
				"name": "CatMode",
				"kind": "ordered",
				"type": "choices={'Scalar','Linear','EsNlcs','DontCat'}",
				"purpose": "返回值拼接选项"
			},
			{
				"name": "Arguments",
				"kind": "ordered",
				"repeating": true,
				"purpose": "输入参数数组"
			},
			{
				"mutuallyExclusiveGroup": [
					[
						{
							"name": "PackDimensions",
							"kind": "namevalue",
							"type": [
								"row",
								"uint8",
								"positive"
							],
							"purpose": "将每个Arguments数组的指定维度打包"
						}
					],
					[
						{
							"name": "SplitDimensions",
							"kind": "namevalue",
							"type": [
								"row",
								"uint8",
								"positive"
							],
							"purpose": "在每个Arguments数组的指定维度上拆分"
						}
					]
				]
			}
		]
	},
	"FolderFun": {
		"inputs": [
			{
				"name": "Function",
				"kind": "required",
				"type": "function_handle",
				"purpose": "要执行的函数句柄"
			},
			{
				"name": "Directory",
				"kind": "required",
				"type": "folder",
				"purpose": "要遍历的文件夹路径"
			},
			{
				"name": "Filename",
				"kind": "ordered",
				"type": "file",
				"purpose": "要筛选出的文件名模式",
				"default": "*"
			},
			{
				"name": "UniformOutput",
				"kind": "namevalue",
				"type": [
					"logical",
					"scalar"
				],
				"purpose": "是否将输出值直接拼接成向量",
				"default": true
			}
		]
	},
	"OpenFileDialog": {
		"inputs": [
			{
				"name": "Filter",
				"kind": "properties",
				"type": [
					"scalar",
					"string"
				],
				"purpose": "文件名筛选器"
			},
			{
				"name": "InitialDirectory",
				"kind": "properties",
				"type": [
					"folder"
				],
				"purpose": "文件对话框中显示的初始目录"
			},
			{
				"name": "Multiselect",
				"kind": "properties",
				"type": [
					"scalar",
					"logical"
				],
				"purpose": "是否允许选择多个文件"
			},
			{
				"name": "Title",
				"kind": "properties",
				"type": [
					"scalar",
					"string"
				],
				"purpose": "文件对话框标题"
			}
		],
		"outputs": [
			{
				"name": "FilePaths",
				"type": [
					"column",
					"string"
				]
			}
		],
		"platforms": "win64"
	}
}