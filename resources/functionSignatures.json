{
	"ParseRepeatingFlagArguments": {
		"inputs": [
			{
				"name": "InputFlags",
				"kind": "required",
				"type": [
					"row",
					"cell"
				],
				"purpose": "调用方传来的，待分析的旗帜类重复参数"
			},
			{
				"name": "ValidFlags",
				"kind": "required",
				"type": [
					"scalar",
					"string"
				],
				"repeating": true,
				"purpose": "函数规定的有效旗帜"
			}
		],
		"outputs": [
			{
				"name": "varargout",
				"type": [
					"scalar",
					"logical"
				],
				"multiplicity": "append"
			}
		]
	},
	"ColorAllocate": {
		"inputs": [
			{
				"name": "NoColors",
				"kind": "required",
				"type": [
					"scalar",
					"uint8"
				],
				"purpose": "要分配的颜色个数"
			},
			{
				"name": "TryCount",
				"kind": "ordered",
				"type": [
					"scalar",
					"uint8"
				],
				"purpose": "尝试优化的次数",
				"default": 1
			}
		],
		"outputs": [
			{
				"name": "Colors",
				"type": [
					"size=:,3"
				]
			}
		]
	},
	"DimensionFun": {
		"inputs": [
			{
				"name": "Function",
				"kind": "required",
				"type": "function_handle",
				"purpose": "要执行的函数。必须接受等同于Arguments重复次数的参数"
			},
			{
				"name": "CatMode",
				"kind": "ordered",
				"type": "choices={'Scalar','Linear','EsNlcs','DontCat'}",
				"purpose": "返回值拼接选项"
			},
			{
				"name": "Arguments",
				"kind": "ordered",
				"repeating": true,
				"purpose": "输入参数数组"
			},
			{
				"mutuallyExclusiveGroup": [
					[
						{
							"name": "PackDimensions",
							"kind": "namevalue",
							"type": [
								"row",
								"uint8",
								"positive"
							],
							"purpose": "将每个Arguments数组的指定维度打包"
						}
					],
					[
						{
							"name": "SplitDimensions",
							"kind": "namevalue",
							"type": [
								"row",
								"uint8",
								"positive"
							],
							"purpose": "在每个Arguments数组的指定维度上拆分"
						}
					]
				]
			}
		]
	}
}