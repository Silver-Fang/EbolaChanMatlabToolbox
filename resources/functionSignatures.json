{
	"ParseRepeatingFlagArguments": {
		"inputs": [
			{
				"name": "InputFlags",
				"kind": "required",
				"type": [
					"row",
					"cell"
				],
				"purpose": "调用方传来的，待分析的旗帜类重复参数"
			},
			{
				"name": "ValidFlags",
				"kind": "required",
				"type": [
					"scalar",
					"string"
				],
				"repeating": true,
				"purpose": "函数规定的有效旗帜"
			}
		],
		"outputs": [
			{
				"name": "varargout",
				"type": [
					"scalar",
					"logical"
				],
				"multiplicity": "append"
			}
		]
	},
	"ColorAllocate":{
		"inputs":[
			{
				"name":"NoColors",
				"kind":"required",
				"type":["scalar","uint8"],
				"purpose":"要分配的颜色个数"
			},
			{
				"name":"TryCount",
				"kind":"ordered",
				"type":["scalar","uint8"],
				"purpose":"尝试优化的次数",
				"default":1
			}
		],
		"outputs":[
			{
				"name":"Colors",
				"type":["size=:,3"]
			}
		]
	}
}